        <frequeny-table>3</frequeny-table>
        <frequeny-table>4</frequeny-table>
        <frequeny-table>99</frequeny-table>
      </transceiver-measurement-result>
    </transceiver-measurement-objects>
    <transceiver-measurement-objects>
      <measurement-object>VOLTAGE</measurement-object>
      <active>true</active>
