    <transceiver-measurement-objects>
      <measurement-object>TEMPERATURE</measurement-object>
      <active>false</active>
      <object-unit>PORT_NUMBER</object-unit>
      <transceiver-measurement-result>
        <object-unit-id>0</object-unit-id>
        <max>
          <value>26.7</value>
          <time>2018-11-27T11:21:00-00:00</time>
        </max>
        <frequeny-table>0</frequeny-table>
      </transceiver-measurement-result>
    </transceiver-measurement-objects>
    <rx-window-measurement-objects>
